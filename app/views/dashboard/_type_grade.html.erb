<div class="row">
	<h2 class="font-weight-light text-muted"><%= results.size %> Mills with Belts Grade: <%= metadata %></h2>
</div>
<div class="row">
	<table class="table table-striped">
		<thead class="thead-dark">
			<tr>
				<th scope="col">Name</th>
				<th scope="col">Quantity</th>
			</tr>
		</thead>

		<tbody>
			<%= render partial: 'type_grade_result', collection: results, as: 'result' %>
		</tbody>
	</table>
</div>
